{
  "name": "parables/json-hyper-schema-cient",
  "description": "A headless client for fetching resources and navigating through hyperlinks provided in the hyper-schema in the response `Content-Type` header",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Parables",
      "email": "38154990+Parables@users.noreply.github.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Parables\\JsonHyperSchema\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "guzzlehttp/guzzle": "^7.8",
    "parables/php-utils": "dev-main",
    "php-jsonpointer/php-jsonpointer": "^4.0",
    "rize/uri-template": "^0.3.5"
  },
  "require-dev": {
    "pestphp/pest": "^2.32",
    "mockery/mockery": "^1.6"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
